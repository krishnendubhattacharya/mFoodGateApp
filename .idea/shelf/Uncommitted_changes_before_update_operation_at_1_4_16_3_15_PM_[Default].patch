+<!--[if gt IE 8]><!--> <html class="no-js"> <!--<![endif]-->
+    <head>
+        <meta charset="utf-8">
+        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
+        <meta name="viewport" content="width=device-width">
+
+        <link rel="stylesheet" href="css/bootstrap.css">
+        <script src="js/jquery-1.9.1.min.js"></script>
+        <script src="../bower_components/angular/angular.js"></script>
+        <script src="../libs/ckeditor/ckeditor.js"></script>
+        <script src="../src/scripts/02-directive.js"></script>
+        <link rel="stylesheet" href="../ng-ckeditor.css">
+    </head>
+<body ng-app="main">
+    <h1>Editor on form</h1>
+
+    <div ng-controller="DemoCtrl">
+
+        <form name="testForm">
+            {{testForm.body.$pristine}}
+            {{test}}
+        <textarea name="body" ckeditor="editorOptions" ng-model="test"></textarea>
+        </form>
+
+        <button ng-click="save()">Save</button>
+
+        <script>
+        var app = angular.module('main', ['ngCkeditor']).
+        controller('DemoCtrl', function($scope, $http) {
+            $scope.editorOptions = {
+                language: 'ru'
+               // uiColor: '#000000'
+            };
+            $scope.save = function() {
+                $http.post('/examples/test.php', {
+                    content: $scope.test
+                }).success(function() {
+                    alert('Saved');
+                });
+            }
+        })
+        </script>
+    </div>
+
+
+    </body>
+</html>
